{
    "version": 2,
    "builds": [
        {
            "src": "webappincome/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "webappincome/wsgi.py"
        }
    ],
    "env": {
        "VERCEL_ENV": "production",
        "DATABASE_URL": "postgresql://postgres.jcycobkzohrufmonwtko:0954852404@aws-1-ap-southeast-1.pooler.supabase.com:5432/postgres"
    }
}